{
  "name": "vite-plugin-codegen",
  "version": "1.1.1",
  "description": "Adds support for codegen files",
  "keywords": [
    "vite",
    "codegen",
    "virtual",
    "module",
    "plugin"
  ],
  "author": "smapiot",
  "homepage": "https://piral.io",
  "license": "MIT",
  "type": "module",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/esm/index.d.ts",
  "engines": {
    "node": ">=16.0"
  },
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/smapiot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smapiot/piral-cli-vite.git"
  },
  "bugs": {
    "url": "https://github.com/smapiot/piral-cli-vite/issues"
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:esm": "tsc --module esnext --outDir lib/esm && sed -i.orig 's/__filename/import\\.meta\\.url/g' lib/esm/index.js"
  },
  "devDependencies": {
    "vite": "^4"
  }
}
